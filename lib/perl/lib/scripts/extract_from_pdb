}
	elsif ( $MODE eq "fasta")
{
	  $n=0;
	  print ">$new_fasta_name\n";
	  
	  foreach $res ( @pdb_seq)
{

	      print "$onelett{$molecule_type{$c}}->{$res}";
              $n++;
	      if ( $n==60){print "\n"; $n=0;}
}
	  print "\n"; 
}
}

if ( $MODE eq "fasta")
{
     &myexit($EXIT_SUCCESS);
  
}

