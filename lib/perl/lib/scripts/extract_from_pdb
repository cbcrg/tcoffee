sub vfopen 
{
    my $file=@_[0];
    my $mode=@_[1];
    my $tmp;
    my $F = new FileHandle;
    
    
    $tmp=$file;
	
    
    if ( $mode eq "r" && !-e $file){ myexit(flush_error ("Cannot open file $file"));}
    elsif ($mode eq "w"){$tmp=">$file";}
    elsif ($mode eq "a"){$tmp=">>$file";}
    
    
    open ($F,$tmp);
    return $F;
}
